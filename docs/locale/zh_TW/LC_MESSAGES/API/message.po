#: ../../API/message.rst:17
msgid "Prelude: Defining related chats"
msgstr ""

#: ../../API/message.rst:19
msgid "master = coordinator.master\n"
"slave = coordinator.slave['ehforwarderbot.channels.slave.eh.demo']\n"
"alice = slave.get_chat(chat_uid=\"alice101\")\n"
"bob = slave.get_chat(chat_uid=\"bobrocks\")\n"
"wonderland = slave.get_chat(chat_uid=\"thewonderlandgroup\")"
msgstr ""

#: ../../API/message.rst:28
msgid "Initialization and marking chats"
msgstr ""

#: ../../API/message.rst:30
msgid "message = EFBMsg()\n\n"
"# 1. a chat delivered from slave to master, or\n"
"message.deliver_to = master\n"
"message.chat = wonderland\n"
"message.author = alice\n\n"
"# 2. a chat delivered from master to slave\n"
"message.deliver_to = slave\n"
"message.chat = alice\n"
"message.author = EFBChat(slave).self()"
msgstr ""

#: ../../API/message.rst:45
msgid "Marking message references (targeted message)"
msgstr ""

#: ../../API/message.rst:47
msgid "# Information in this part should be retrieved\n"
"# from recorded historical data.\n"
"message.target = EFBMsg()\n"
"message.target.chat = alice\n"
"message.target.author = alice\n"
"message.target.text = \"Hello, world.\"\n"
"message.target.type = MsgType.Text\n"
"message.target.uid = \"100000002\""
msgstr ""

#: ../../API/message.rst:59
msgid "Edit a previously sent message"
msgstr ""

#: ../../API/message.rst:61
msgid "message.edit = True\n"
"message.uid = \"100000003\"\n"
"# Message UID should be the UID from the slave channel\n"
"# regardless of where the message is delivered to."
msgstr ""

