msgid "@-reference the User Themself, another member in the same chat, and the entire chat in the message text."
msgstr ""

#: /Users/blueset/Codebase/ehForwarderBot/docs/API/message.rst:180
#: d82c1b3e161a4198970452d771699fd5
msgid "message.text = \"Hey @david, @bob, and @all. Attention!\"\n"
"message.substitutions = Substitutions({\n"
"    # text[4:10] = \"@david\", here David is the user.\n"
"    (4, 10): wonderland.self,\n"
"    # text[12:16] = \"@bob\", Bob is another member of the chat.\n"
"    (12, 16): wonderland.get_member(\"bob\"),\n"
"    # text[22:126] = \"@all\", this calls the entire group chat, hence the\n"
"    # chat object is set as the value instead.\n"
"    (22, 26): wonderland\n"
"})"
msgstr ""

